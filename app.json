{
  "expo": {
    "name": "meditation",
    "slug": "meditation",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/simpleMeditationLogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/simpleMeditationLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/simpleMeditationLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mcrusher90.meditation"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "b6b5d263-6e6c-43ac-b022-5edacb0c8fd9"
      }
    }
  }
}
